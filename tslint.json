{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "indent": [true, "tabs"],
        "no-var-requires": false,
        "trailing-comma": [true, {"multiline": "never", "singleline": "never"}]
    },
    "rulesDirectory": []
}
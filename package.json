{
  "name": "typescript-react-redux-scaffold",
  "version": "1.0.0",
  "description": "Starter pack for building applications with typescript react and redux",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --content-base ./dist --hot --inline --colors --port 8080 --open",
    "start-code": "webpack-dev-server --content-base ./dist --hot --progress --colors --host 0.0.0.0 --devtool eval --port 8080",
    "start-cloud": "webpack-dev-server  --host $IP --port $PORT  --hot --progress",
    "start-server": "node server.js --host $IP --port $PORT",
    "build": "webpack --config webpack.config.js ",
    "dev": "npm run build && npm run start",
    "cloud": "npm run build && npm run start-cloud",
    "code": "npm run build && npm run start-code"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orlando-paredes-hamsho/typescript-react-redux-scaffold.git"
  },
  "keywords": [
    "react",
    "redux",
    "typescript"
  ],
  "author": "Orlando Paredes Hamsho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/orlando-paredes-hamsho/typescript-react-redux-scaffold/issues"
  },
  "homepage": "https://github.com/orlando-paredes-hamsho/typescript-react-redux-scaffold#readme",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "source-map-loader": "^0.2.0",
    "ts-loader": "^2.0.3",
    "typescript": "^2.2.2",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "@types/react": "^15.0.20",
    "@types/react-dom": "^0.14.23",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
